    "fathom-client": "^3.6.0",
    "feed": "^4.2.2",
    "front-matter": "^4.0.2",
    "isbot": "^5",
    "lru-cache": "^10.1.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-router": "7.12.0",
