{
  "name": "remix-website",
  "version": "30.1.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "clean": "git clean -fdX .",
    "clean:safe": "del public/build .cache build prisma/dev.db",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "npm run build:css && remix build",
    "dev": "pm2-dev pm2.config.js",
    "dev:docs": "node -r ts-node/register/transpile-only -r dotenv/config scripts/watcher.ts",
    "format": "prettier --write ./",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "NODE_ENV=production remix-serve build",
    "db:reset": "prisma migrate reset",
    "seed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config  scripts/seed.ts",
    "postseed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config scripts/verify-seed.ts"
  },
  "dependencies": {
    "@docsearch/css": "^3.0.0-alpha.41",
    "@docsearch/react": "^3.0.0-alpha.41",
    "@mcansh/undoc": "^0.5.0",
    "@prisma/client": "^3.5.0",
    "@reach/menu-button": "0.17.0",
    "@reach/rect": "0.17.0",
    "@reach/tabs": "0.17.0",
    "@remix-run/node": "1.5.1",
    "@remix-run/react": "1.5.1",
    "@remix-run/serve": "1.5.1",
    "@ryanflorence/md": "^7.1.0",
    "@ryanflorence/mdtut": "^1.2.1",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "accept-language": "^3.0.18",
    "clsx": "^1.1.1",
    "cssnano": "^5.0.11",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "emoji-regex": "^10.1.0",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "lru-cache": "^6.0.0",
    "p-queue": "^6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^13.0.0",
    "remark-html": "13.0.1",
    "remix": "1.5.1",
    "semver": "^7.3.5",
    "ts-invariant": "^0.9.3",
    "ts-is-present": "^1.2.2",
    "ts-node": "^10.4.0",
    "tween-functions": "^1.2.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.5.1",
    "@types/lru-cache": "^5.1.1",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/semver": "^7.3.9",
    "chokidar": "^3.5.2",
    "del-cli": "^4.0.1",
    "fathom-client": "^3.4.1",
    "pm2": "^5.1.2",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-focus-visible": "^6.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.0.2",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16"
  },
  "prettier": {}
}
