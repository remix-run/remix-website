{
  "name": "remix-website",
  "version": "30.1.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:analyze": "ANALYZE=true react-router build",
    "dev": "vite dev",
    "format": "prettier --write ./",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "push:stage": "git tag -f stage && git push origin stage -f",
    "start": "cross-env NODE_ENV=production node server.ts",
    "preview": "cross-env NODE_ENV=production node --env-file=.env server.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint -- --fix",
    "test": "vitest",
    "pretest:e2e": "playwright install chromium",
    "test:e2e": "playwright test",
    "typecheck": "react-router typegen && tsc --noEmit -p remix/tsconfig.json && tsc --noEmit -p app/tsconfig.json"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/react": "^3.5.2",
